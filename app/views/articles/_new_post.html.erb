<div class="post-outline container">
  <div class="post-details row">
    <span class="col-md-4"><%= "#{User.find(post.user_id).first_name} #{User.find(post.user_id).last_name}" %></span>
    <span class="col-md-4"><%= post.id %></span>
    <span class="col-md-4"><%= post.created_at %></span>
  </div>
  <br>
  <div class="post-text">
    <%= post.text %>
  </div>
</div>
<br>
